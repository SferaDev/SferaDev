{
	"name": "nuki-api-js",
	"version": "1.0.0",
	"description": "Nuki auto-generated type-safe API client",
	"type": "module",
	"author": "SferaDev",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SferaDev/SferaDev.git"
	},
	"bugs": {
		"url": "https://github.com/SferaDev/SferaDev/issues"
	},
	"homepage": "https://github.com/SferaDev/SferaDev#readme",
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "bunchee",
		"generate": "rimraf ./src/generated && kubb generate",
		"is-tree-shakable": "is-tree-shakable"
	},
	"dependencies": {
		"@sferadev/openapi-utils": "workspace:*",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@kubb/cli": "catalog:",
		"@kubb/core": "catalog:",
		"effect": "catalog:",
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"bunchee": "catalog:",
		"case": "catalog:",
		"openapi3-ts": "catalog:",
		"rimraf": "catalog:",
		"typescript": "catalog:"
	},
	"peerDependencies": {
		"effect": "^3.0.0"
	},
	"peerDependenciesMeta": {
		"effect": {
			"optional": true
		}
	},
	"files": [
		"dist"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./types": {
			"types": "./dist/types.d.mts",
			"import": "./dist/types.mjs",
			"require": "./dist/types.cjs"
		},
		"./schemas": {
			"types": "./dist/schemas.d.mts",
			"import": "./dist/schemas.mjs",
			"require": "./dist/schemas.cjs"
		},
		"./components": {
			"types": "./dist/components.d.mts",
			"import": "./dist/components.mjs",
			"require": "./dist/components.cjs"
		},
		"./effect": {
			"types": "./dist/effect.d.mts",
			"import": "./dist/effect.mjs",
			"require": "./dist/effect.cjs"
		}
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
