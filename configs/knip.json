{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"workspaces": {
		".": {
			"entry": ["turbo.json"]
		},
		"apps/website": {
			"entry": ["app/**/*.{ts,tsx}", "lib/**/*.ts", "components/**/*.tsx"],
			"project": ["**/*.{ts,tsx}"],
			"ignore": ["**/*.d.ts"]
		},
		"packages/ai-gateway-proxy": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.{ts,tsx}"]
		},
		"packages/rollup-plugin-import-cdn": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/openapi-utils": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.{ts,tsx}"]
		},
		"packages/vercel-api-js": {
			"entry": ["src/index.ts", "src/mcp.ts"],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"]
		},
		"packages/v0-api": {
			"entry": ["src/index.ts", "src/mcp.ts"],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"]
		},
		"packages/cloudflare-api": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/netlify-api": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/zoom-api-js": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/keycloak-api": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/nuki-api-js": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/litellm-api": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"configs/tsconfig": {}
	},
	"ignoreWorkspaces": ["configs/tsconfig"]
}
