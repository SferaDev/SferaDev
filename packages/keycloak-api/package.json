{
	"name": "keycloak-api",
	"version": "0.2.6",
	"description": "Keycloak auto-generated type-safe API client",
	"type": "module",
	"author": "SferaDev",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SferaDev/SferaDev.git"
	},
	"bugs": {
		"url": "https://github.com/SferaDev/SferaDev/issues"
	},
	"homepage": "https://github.com/SferaDev/SferaDev#readme",
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "bunchee",
		"generate": "rimraf ./src/admin/generated ./src/account/generated && kubb generate",
		"is-tree-shakable": "is-tree-shakable"
	},
	"dependencies": {
		"zod": "catalog:"
	},
	"devDependencies": {
		"@kubb/cli": "catalog:",
		"@kubb/core": "catalog:",
		"@modelcontextprotocol/sdk": "catalog:",
		"@sferadev/openapi-utils": "workspace:*",
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"bunchee": "catalog:",
		"case": "catalog:",
		"openapi3-ts": "catalog:",
		"rimraf": "catalog:",
		"typescript": "catalog:",
		"yaml": "catalog:"
	},
	"peerDependencies": {
		"@modelcontextprotocol/sdk": "^1.15.1"
	},
	"peerDependenciesMeta": {
		"@modelcontextprotocol/sdk": {
			"optional": true
		}
	},
	"files": [
		"dist"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./admin/types": {
			"types": "./dist/admin/types.d.mts",
			"import": "./dist/admin/types.mjs",
			"require": "./dist/admin/types.cjs"
		},
		"./admin/schemas": {
			"types": "./dist/admin/schemas.d.mts",
			"import": "./dist/admin/schemas.mjs",
			"require": "./dist/admin/schemas.cjs"
		},
		"./admin/components": {
			"types": "./dist/admin/components.d.mts",
			"import": "./dist/admin/components.mjs",
			"require": "./dist/admin/components.cjs"
		},
		"./account/types": {
			"types": "./dist/account/types.d.mts",
			"import": "./dist/account/types.mjs",
			"require": "./dist/account/types.cjs"
		},
		"./account/schemas": {
			"types": "./dist/account/schemas.d.mts",
			"import": "./dist/account/schemas.mjs",
			"require": "./dist/account/schemas.cjs"
		},
		"./account/components": {
			"types": "./dist/account/components.d.mts",
			"import": "./dist/account/components.mjs",
			"require": "./dist/account/components.cjs"
		}
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
