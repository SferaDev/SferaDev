{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**", "out/**"],
			"dependsOn": ["^build"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"generate": {
			"inputs": ["kubb.config.ts", "openapi-codegen.config.ts"],
			"outputs": ["src/generated/**"],
			"dependsOn": ["^build"]
		},
		"tsc": {},
		"test": {},
		"clean": {
			"cache": false
		}
	}
}
