{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@sferadev/tsconfig/base.json",
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"types": ["node"],
		"noEmit": true,
		"jsx": "react-jsx",
		"jsxImportSource": "@kubb/react"
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
}
