    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@vercel/oidc": "^3.0.3"
  },
  "devDependencies": {
    "@sferadev/eslint-config": "workspace:*",
    "@sferadev/tsconfig": "workspace:*",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}