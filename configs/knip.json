{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"workspaces": {
		".": {
			"entry": ["turbo.json", ".github/workflows/scripts/*.ts"]
		},
		"apps/website": {
			"entry": ["app/**/*.{ts,tsx}", "lib/**/*.ts", "components/**/*.tsx", "vercel.ts"],
			"project": ["**/*.{ts,tsx}"],
			"ignore": ["**/*.d.ts"]
		},
		"apps/photocall": {
			"entry": [
				"app/**/*.{ts,tsx}",
				"components/**/*.tsx",
				"lib/**/*.ts",
				"hooks/**/*.ts",
				"convex/**/*.ts",
				"scripts/**/*.ts"
			],
			"project": ["**/*.{ts,tsx}"],
			"ignore": ["**/*.d.ts", "convex/_generated/**"],
			"ignoreDependencies": ["jszip"],
			"includeEntryExports": false
		},
		"apps/openapi": {
			"entry": ["src/app/**/*.{ts,tsx}", "src/lib/**/*.ts", "src/components/**/*.tsx"],
			"project": ["src/**/*.{ts,tsx}"],
			"ignore": ["**/*.d.ts"],
			"ignoreDependencies": ["tailwindcss", "tw-animate-css"]
		},
		"apps/vscode-ai-gateway": {
			"project": ["src/**/*.ts"],
			"ignoreDependencies": ["@types/json-schema"]
		},
		"packages/ai-gateway-proxy": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.{ts,tsx}"]
		},
		"packages/rollup-plugin-import-cdn": {
			"entry": ["src/index.ts"],
			"project": ["src/**/*.ts"]
		},
		"packages/openapi-utils": {
			"entry": ["src/index.ts", "src/effect/index.ts"],
			"project": ["src/**/*.{ts,tsx}"],
			"ignoreDependencies": ["@types/react", "@types/react-dom", "react", "react-dom", "effect"]
		},
		"packages/vercel-api-js": {
			"entry": [
				"src/index.ts",
				"src/types.ts",
				"src/schemas.ts",
				"src/components.ts",
				"src/effect.ts",
				"kubb.config.ts"
			],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"],
			"includeEntryExports": false
		},
		"packages/v0-api": {
			"entry": [
				"src/index.ts",
				"src/types.ts",
				"src/schemas.ts",
				"src/components.ts",
				"src/effect.ts",
				"kubb.config.ts"
			],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"],
			"includeEntryExports": false
		},
		"packages/cloudflare-api": {
			"entry": ["src/index.ts", "src/types.ts", "src/components.ts", "kubb.config.ts"],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"],
			"includeEntryExports": false
		},
		"packages/netlify-api": {
			"entry": [
				"src/index.ts",
				"src/types.ts",
				"src/schemas.ts",
				"src/components.ts",
				"kubb.config.ts"
			],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"],
			"includeEntryExports": false
		},
		"packages/zoom-api-js": {
			"entry": [
				"src/index.ts",
				"src/types.ts",
				"src/schemas.ts",
				"src/components.ts",
				"kubb.config.ts"
			],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"],
			"includeEntryExports": false
		},
		"packages/keycloak-api": {
			"entry": [
				"src/index.ts",
				"src/admin/types.ts",
				"src/admin/schemas.ts",
				"src/admin/components.ts",
				"src/account/types.ts",
				"src/account/schemas.ts",
				"src/account/components.ts",
				"kubb.config.ts"
			],
			"project": ["src/**/*.ts"],
			"ignore": ["src/admin/generated/**", "src/account/generated/**"],
			"includeEntryExports": false
		},
		"packages/nuki-api-js": {
			"entry": [
				"src/index.ts",
				"src/types.ts",
				"src/schemas.ts",
				"src/components.ts",
				"kubb.config.ts"
			],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"],
			"includeEntryExports": false
		},
		"packages/litellm-api": {
			"entry": [
				"src/index.ts",
				"src/types.ts",
				"src/schemas.ts",
				"src/components.ts",
				"kubb.config.ts"
			],
			"project": ["src/**/*.ts"],
			"ignore": ["src/generated/**"],
			"includeEntryExports": false
		},
		"configs/tsconfig": {}
	},
	"ignoreWorkspaces": ["configs/tsconfig"],
	"ignoreExportsUsedInFile": true,
	"rules": {
		"duplicates": "off",
		"optionalPeerDependencies": "off"
	}
}
