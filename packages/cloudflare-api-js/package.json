{
	"name": "cloudflare-api-js",
	"version": "0.18.1",
	"description": "Cloudflare auto-generated type-safe API client",
	"type": "module",
	"author": "SferaDev",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SferaDev/SferaDev.git"
	},
	"bugs": {
		"url": "https://github.com/SferaDev/SferaDev/issues"
	},
	"homepage": "https://github.com/SferaDev/SferaDev#readme",
	"scripts": {
		"tsc": "tsc --noEmit",
		"build": "bunchee",
		"generate": "if [ -d ./src/generated ] && [ -n \"$(find ./src/generated -maxdepth 0 -mmin -300 2>/dev/null)\" ]; then echo 'Generated files are fresh, skipping. Run generate:force to regenerate.'; else rimraf ./src/generated && kubb generate; fi",
		"generate:force": "rimraf ./src/generated && kubb generate",
		"is-tree-shakable": "is-tree-shakable"
	},
	"dependencies": {},
	"devDependencies": {
		"@kubb/cli": "catalog:",
		"@kubb/core": "catalog:",
		"@sferadev/openapi-utils": "workspace:*",
		"@sferadev/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"bunchee": "catalog:",
		"case": "catalog:",
		"openapi3-ts": "catalog:",
		"rimraf": "catalog:",
		"typescript": "catalog:"
	},
	"files": [
		"dist"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./types": {
			"types": "./dist/types.d.mts",
			"import": "./dist/types.mjs",
			"require": "./dist/types.cjs"
		},
		"./components": {
			"types": "./dist/components.d.mts",
			"import": "./dist/components.mjs",
			"require": "./dist/components.cjs"
		}
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
